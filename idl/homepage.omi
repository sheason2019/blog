import { Article, Album } from "./blog.omi";

service HomePage {
  // 获取文章
  repeated Article GetArticles(
    int length,
    int offset,
    boolean GetNew,
    repeated int SectionsId,
  );
  // 获取合集
  repeated Album GetAlbums(
    int length,
    int offset,
    boolean GetNew,
  );
  Album GetAlbum(int id);
}